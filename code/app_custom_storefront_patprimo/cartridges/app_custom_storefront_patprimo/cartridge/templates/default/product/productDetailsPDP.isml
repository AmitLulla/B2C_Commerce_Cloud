<iscomment><isdecorate template="common/layout/page">

    <isscript>
        var assets = require('*/cartridge/scripts/assets');
        assets.addJs('/js/productDetail.js');
        assets.addCss('/css/product/detail.css');
    </isscript>

    <isset name="product" value="${pdict.product}" scope="page" />
    <isset name="isQuickView" value="${false}" scope="page" />
    <isset name="isProductSet" value="${pdict.product.productType === 'set'}" scope="page" />
    <isset name="customPDP" value="${pdict.product.customPDP}" scope="page" />
    <isset name="productVariant" value="${pdict.productVariant}" scope="page" />
    <script>
        dataLayer.push({
            event: "view_item",
            ecommerce: {
            items: [{
                item_name: ${"'"+pdict.product.productName+"'"},
                item_id: ${"'"+pdict.prodObj.item_id+"'"},
                item_sku_id: ${"'"+pdict.prodObj.item_sku_id+"'"},
                final_price: ${pdict.prodObj.final_price != '' ? pdict.prodObj.final_price.toFixed(): "''"},
                original_price: ${pdict.prodObj.original_price != '' ? pdict.prodObj.original_price : "''"},
                item_category: ${"'"+pdict.prodObj.item_category+"'"},
                item_category2: ${"'"+pdict.prodObj.item_category2+"'"},
                size: ${"'"+pdict.prodObj.item_size+"'"},
                available_size: ${"'"+pdict.prodObj.available_size+"'"},
                color: ${"'"+pdict.prodObj.item_color+"'"}
            }]
            }
        });
    </script>
    <isobject object="${product.raw}" view="detail" >
        <div class="container product-detail product-wrapper" data-pid="${product.id}">
            <div class="row">
                <div class="col-12">
                    <!--Breadcrumbs-->
                    <div class="product-breadcrumb d-md-none">
                        <isinclude template="components/breadcrumbs/pageBreadcrumbs"/>
                    </div>

                    <!-- Product Name -->
                    <div class="row">
                        <div class="d-md-none col-sm-12">
                            <h1 class="product-name">${product.productName}</h1>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <!-- Product Images Carousel -->
                <isif condition="${productVariant.productType == 'variant'}">
                    <isinclude template="product/components/imageCarouselPDP" />
                <iselse/>
                    <isinclude template="product/components/imageCarousel" />
                </isif> 

                <div class="col-12 col-sm-6">
                    <!--Breadcrumbs-->
                    <div class="row">
                        <div class="product-breadcrumb col hidden-sm-down">
                            <isinclude template="components/breadcrumbs/pageBreadcrumbs"/>
                        </div>
                    </div>

                    <!-- Product Name -->
                    <div class="row">
                        <div class="col">
                            <h1 class="product-name hidden-sm-down">${product.productName}</h1>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col">
                            <isinclude template="product/components/pidRating" />
                        </div>
                    </div>

                    <!-- Custom pricing -->
                    <div class="row">
                        <div class="col-12 prices ${(product.customPDP === 'true') ? 'custom_attr_price' : ''}">
                            <!-- Prices -->
                            <isset name="price" value="${product.price}" scope="page" />
                            <isinclude template="product/components/pricing/main" />
                        </div>
                    </div>

                    <!-- Custom - Base product attributes -->
                    <div class="attributes custom-pdp-items">
                        
                        <!-- Attributes and Quantity -->
                        <isinclude template="product/components/mainAttributes" />

                        <div class="row text-center">
                            <div class="col-4">
                                <!-- Select <Attribute> Label -->
                                <span class="${attr.id} non-input-label">
                                    ${Resource.msg('label.color', 'product', null)}           
                                </span>
                            </div>
                            <div class="col-4">
                                <span class="${attr.id} non-input-label">
                                    ${Resource.msg('label.size', 'product', null)}            
                                </span>
                            </div>
                            <div class="col-4">
                                <span class="${attr.id} non-input-label">
                                    ${Resource.msg('label.quantity', 'product', null)}            
                                </span>
                            </div>
                        </div> 

                        <isset name="isBundle" value="${false}" scope="page" />
                        <isset name="loopState" value="${{count: 1}}" scope="page" />
                        
                        <isloop items="${product.variationAttributes}" var="attr" status="attributeStatus">
                            <isif condition="${attr.id == 'color' && attr.swatchable}">
                                <span id="idCustomPDP" class="d-none">${customPDP}</span>
                                <span id="idProd" class="d-none">${product.id}</span>
                                <isloop items="${attr.values}" var="attrValue" status="attributeStatus">
                                    <div class="row" data-attr="${attr.id}">
                                        <div class="col-12 p-0 custom-pdp-item" data-pid="${product.id}">                                        
                                            <isinclude template="product/components/customVariationAttribute" />
                                        </div>
                                    </div>
                                </isloop>
                            </isif>                                
                        </isloop>

                        <isif condition="${!product.variationAttributes}">
                            <div class="row">
                                <div class="col-12">
                                    <div class="simple-quantity">
                                        <isinclude template="product/components/quantity" />
                                    </div>
                                </div>
                            </div>
                        </isif>

                        <div class="product-options">
                            <isif condition="${product.options && product.options.length > 0}">
                                <isinclude template="product/components/options" />
                            </isif>
                        </div>

                        <!-- Size Chart -->
                        <isif condition="${product.variationAttributes && product.sizeChartId}">
                            <div class="row">
                                <div class="size-chart col-12">
                                    <isinclude template="product/components/sizeChart" />
                                </div>
                            </div>
                        </isif>

                        <!-- Product Availability -->
                        <iscomment> <isinclude template="product/components/productAvailability" /> </iscomment>

                        <!-- Custom selected products and quantities -->
                    <iscomment>    <isinclude template="product/components/customSelectedProducts" />
                        </br>
                        <!-- Applicable Promotions -->
                        <div class="row">
                            <div class="col-12 promotions">
                                <isinclude template="product/components/promotions" />
                            </div>
                        </div>

                        <div class="prices-add-to-cart-actions">
                            <!-- Cart and [Optionally] Apple Pay -->
                            <isinclude template="product/components/addToCartGlobal" />
                        </div>

                        <!-- Social Sharing Icons -->
                        <isif condition="${!product.variationAttributes}">
                            <div class="row">
                                <isinclude template="product/components/socialIcons" />
                            </div>
                        </isif>
                    </div>
                </div>
            </div>

            <div>
                <isslot id="cts-recommendations-m" description="Complete the set products" context="global" context-object="${pdict.product.raw}" />
            </div>

            <isset name="loopState" value="${{count: 1}}" scope="page" />
            <isinclude template="product/components/descriptionAndDetails" />

            <div class="recommendations">
                <isslot id="product-recommendations-m" description="Recommended products" context="global" context-object="${pdict.product.raw}" />
            </div>
        </div>
    </isobject>
</isdecorate>
</iscomment>

