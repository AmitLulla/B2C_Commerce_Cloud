const mobileDiv = document.querySelector(".select-menu-mobile-pdp");
const galleria = document.querySelector(".galerry-pdp"); 
const selectPdp = document.querySelector(".select-pdp");
// console.log(mobileDiv);
let lastKnownScrollPosition = 0;
let active_slide = false;

document.addEventListener("scroll",function(){
lastKnownScrollPosition = window.scrollY;
var x = window.matchMedia("(max-width: 700px)");
//  console.log(x);
  if (x.matches) { 
if(lastKnownScrollPosition > 100){
    active_slide = true;
     mobileDiv.classList.add("main-mobile-active");
    galleria.classList.add("main-mobile-active-gallery");
    selectPdp.classList.add("main-mobile-active-select");
}
else {
    active_slide=false;
     mobileDiv.classList.remove("main-mobile-active");
    galleria.classList.remove("main-mobile-active-gallery");
    selectPdp.classList.remove("main-mobile-active-select");
}
  }

